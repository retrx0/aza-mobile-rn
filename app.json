{
  "expo": {
    "name": "aza",
    "slug": "aza",
    "currentFullName": "@retro1/aza",
    "originalFullName": "@retro1/aza",
    "version": "0.1.1",
    "orientation": "portrait",
    "primaryColor": "#000000",
    "icon": "./assets/images/app/aza-app-icon-light.png",
    "scheme": "com.aza.azaapp",
    "owner": "retro1",
    "notification": {},
    "splash": {
      "image": "./assets/images/app/aza-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/c56fed50-115a-45bd-9836-6c7f739bd420"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "usesIcloudStorage": false,
      "bundleIdentifier": "com.aza.azaapp",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/app/aza-app-icon-light.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aza.azaapp",
      "googleServicesFile": "google-services.json"
    },
    "web": {
      "favicon": "./assets/images/app/aza-app-icon-light.png"
    },
    "extra": {
      "eas": {
        "projectId": "c56fed50-115a-45bd-9836-6c7f739bd420"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      "sentry-expo",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/app/aza-app-icon-light.png",
          "color": "#ffffff",
          "sounds": "./assets/sounds/notifications/coin-drop.wav"
        }
      ],
      "@config-plugins/detox"
    ]
  },
  "infoPlist": {
    "NSFaceIDUsageDescription": "Authentication/authorization"
  }
}
