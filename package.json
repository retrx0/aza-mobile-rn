  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest --watchAll --coverage=false --changedSince=origin/main",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --fix --ext .ts",
    "prettier": "yarn run prettier src test --check",
    "detox ios debug": "detox test --configuration ios.sim.debug",
    "detox android debug": "detox test --configuration android.emu.debug",
    "detox ios release": "detox test --configuration ios.sim.release",
    "detox android release": "detox test --configuration android.emu.release"
